(x'=0∧same(x)∧pc=L0∧pc'=L1) ∨
(y'=0∧same(y)∧pc=L1∧pc'=L2) ∨
(z'=0∧same(z)∧pc=L2∧pc'=L3) ∨
(x'=y+1∧same(x)∧pc=L3∧pc'=L4) ∨
(z'=z+2∧same(z)∧pc=L4∧pc'=L5) ∨
while:
(pc=L5∧pc'=W1∧(y<3)∧same(V)) ∨
(pc=L5∧pc'=L7∧¬(y<3)∧same(V)) ∨
if:
(pc=L₁∧pc'=L₁₁∧(x<y)∧same(V)) ∨
(pc=L₁∧pc'=L₂₂∧¬(x<y)∧same(V)) ∨

(pc=L₁₁∧pc'=L6∧(x=x+1)∧same(x)) ∨
(pc=L₂₂∧pc'=L6∧(y=y+1)∧same(y)) ∨

(pc=L6∧pc'=L7∧(y=y+1)∧same(y)) ∨


